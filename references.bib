@Article{barrett09integrated,
  Title                    = {An Integrated Modeling Environment to Study the
 Co-evolution of Networks, Individual Behavior, and
 Epidemics},
  Author                   = {C. Barrett and K. Bisset and J. Leidig and A. Marathe and
 M. Marathe},
  Journal                  = {AI Magazine},
  Year                     = {2009},
  Number                   = {1},
  Pages                    = {75-87},
  Volume                   = {31},

  Abstract                 = {We discuss an interaction-based approach to study the
 co-evolution between socio-technical networks, individual
 behaviors and contagion processes on these networks. We use
 epidemics in human population as an exemplar of this
 phenomenon. The methods consist of developing synthetic yet
 realistic national-scale networks using a first principles
 approach. Unlike simple random graph techniques, these
 methods use real world data sources and combine them with
 behavioral and social theories to synthesize networks.
 Individual-based models of disease progression and
 transmission are then used to model the contagion process.
 Finally, models of individual behaviors are composed with
 disease progression models to develop a realistic
 representation of the complex system in which individual
 behaviors and the social network adapt to the contagion.
 These ideas are embodied within a general purpose modeling
 environment called Simdemics. Simdemics is designed
 specifically to be scalable to networks with 300 million
 agents - the underlying algorithms and methods in Simdemics
 are all high-performance computing oriented methods. New
 advances in network science, machine learning, high
 performance computing, data mining and behavioral modeling
 were necessary to develop Simdemics.

 Simdemics is combined with two other environments:
 Simfrastructure and Didactic to form an integrated
 cyber-environment. The integrated cyber-environment
 provides the end-user flexible and seamless Internet based
 access to the high-performance computing models represented
 in Simdemics. Service-oriented architectures plays critical
 role in delivering the desired services to the end user.
 Simdemics and the integrated cyber-environment have been
 used in over a dozen user defined case studies. These case
 studies were done to support specific policy questions that
 arose in the context of planning and responding in the
 event of pandemics (e.g. H1N1, avian influenza) or other
 human initiated bio-terrorism events. These studies played
 a crucial role in the continual development and improvement
 of the cyber-environment.},
  File                     = {:ndssl/barrett09.an-integrated-modeling-environment-to-study-the-coevolution-of-networks-individual-behavior-and-epidemics.pdf:PDF},
  Keywords                 = {ndssl, coevolution, complex networks, synthetic
 information environments, behavior, epidemics},
  Owner                    = {swarup},
  Timestamp                = {2009.09.08}
}


@Article{clark98extendedmind,
  Title                    = {The Extended Mind},
  Author                   = {Andy Clark and David Chalmers},
  Journal                  = {Analysis},
  Year                     = {1998},

  Month                    = {Jan},
  Number                   = {1},
  Pages                    = {7-19},
  Volume                   = {58},

  Abstract                 = {Where does the mind stop and the rest of the world begin?
 The question invites two standard replies. Some accept the
 boundaries of skin and skull, and say that what is outside
 the body is outside the mind. Others are impressed by
 arguments suggesting that the meaning of our words `just
 ain't in the head', and hold that this externalism about
 meaning carries over into an externalism about mind. We
 propose to pursue a third position. We advocate a very
 different sort of externalism: an active externalism, based
 on the active role of the environment in driving cognitive
 processes.},
  Keywords                 = {cognition, philosophy of mind, philosophy of cognition,
 extended mind, extended cognition},
